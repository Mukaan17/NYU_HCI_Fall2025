{
  "expo": {
    "name": "Violet Vibes",
    "slug": "VioletVibes",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0b132b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.violetvibes.app",
      "scheme": "violetvibes",
      "icon": "./media/App Icon.png"
    },
    "android": {
      "package": "com.violetvibes.app"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow VioletVibes to use your location to find awesome spots near you."
        }
      ],
      "expo-notifications"
    ]
  }
}